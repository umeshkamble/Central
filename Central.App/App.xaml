                <Setter Property="PnPromo" Value="{Binding Promo}"/>
                <Setter Property="PnBayar" Value="{Binding Bayar}"/>
                <Setter Property="PnSisa" Value="{Binding Sisa}"/>
                <Setter Property="PnKembalian" Value="{Binding Kembalian}"/>
                <Setter Property="PnSplit" Value="{Binding IsSplit}"/>
                <Setter Property="PnIncludePoint" Value="{Binding IncludePoint}"/>
                <Setter Property="PnPromoText" Value="{Binding PromoText}"/>

                <Setter Property="PnInputSwitchSplitVM" Value="{Binding InputSwitchSplitVM}"/>
                <Setter Property="PnInputSwitchPointVM" Value="{Binding InputSwitchPointVM}"/>
