<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Central.App"
             xmlns:temp="clr-namespace:Central.App.Templates"
             xmlns:view="clr-namespace:Central.App.Views"
             x:Class="Central.App.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>


            <ResourceDictionary Source="Themes/ThemeLight.xaml" />

            <!--LabelStyle-->
            <Style x:Key="LabelStyle" TargetType="Label">
                <Setter Property="FontFamily" Value="FontRegular"/>
                <Setter Property="LineBreakMode" Value="WordWrap"/>
            </Style>
            <Style TargetType="temp:LabelTitleV" BasedOn="{StaticResource LabelStyle}">
                <Setter Property="TextColor" Value="{DynamicResource ClrPrimary}"/>
                <Setter Property="FontFamily" Value="FontTitle"/>
                <Setter Property="FontSize" Value="Large"/>
            </Style>
            <Style TargetType="temp:LabelNormalV" BasedOn="{StaticResource LabelStyle}">
                <Setter Property="FontSize" Value="Small"/>
            </Style>
            <Style TargetType="temp:LabelHintV" BasedOn="{StaticResource LabelStyle}">
                <Setter Property="FontSize" Value="Small"/>
                <Setter Property="TextColor" Value="{DynamicResource ClrTextHint}"/>
            </Style>
            <Style TargetType="temp:LabelIconV" BasedOn="{StaticResource LabelStyle}">
                <Setter Property="FontSize" Value="Large"/>
                <Setter Property="FontFamily" Value="FontAwesome"/>
                <Setter Property="VerticalOptions" Value="Center"/>
                <Setter Property="TextColor" Value="{DynamicResource ClrPrimary}"/>
            </Style>


            <!--EntryStyle-->
            <Style TargetType="temp:EntryV">
                <Setter Property="FontFamily" Value="FontRegular"/>
                <Setter Property="FontSize" Value="Small"/>
            </Style>

            <!--EditorStyle-->
            <Style TargetType="temp:EditorV">
                <Setter Property="FontFamily" Value="FontRegular"/>
                <Setter Property="FontSize" Value="Small"/>
            </Style>

            <!--DatePickerStyle-->
            <Style TargetType="temp:DatePickerV">
                <Setter Property="FontFamily" Value="FontRegular"/>
                <Setter Property="FontSize" Value="Small"/>
            </Style>

            <!--SwitchStyle-->
            <Style TargetType="temp:SwitchV"/>

            <!--ButtonStyle-->
            <Style x:Key="ButtonStyle" TargetType="temp:ButtonV">
                <Setter Property="BackgroundColor" Value="{DynamicResource ClrPrimary}"/>
                <Setter Property="TextColor" Value="White"/>
                <Setter Property="FontFamily" Value="FontTitle"/>
                <Setter Property="FontSize" Value="Medium"/>
                <Setter Property="CornerRadius" Value="5"/>
            </Style>
            <Style TargetType="temp:ButtonNormalV" BasedOn="{StaticResource ButtonStyle}">
            </Style>

            <!--SearchBarStyle-->
            <Style TargetType="temp:SearchBarV">
                <Setter Property="Margin" Value="0,-5,0,-5"/>
                <Setter Property="FontFamily" Value="FontRegular"/>
                <Setter Property="FontSize" Value="Small"/>
                <Setter Property="VerticalOptions" Value="CenterAndExpand"/>
                <Setter Property="HorizontalOptions" Value="FillAndExpand"/>
            </Style>

            <!--FrameStyle-->
            <Style TargetType="temp:FrameV">
                <Setter Property="CornerRadius" Value="10"/>
                <Setter Property="HasShadow" Value="False"/>
                <Setter Property="VerticalOptions" Value="Center"/>
            </Style>

            <!--ImageStyle-->
            <Style x:Key="ImageStyle" TargetType="Image">
                <Setter Property="Aspect" Value="AspectFill"/>
                <Setter Property="VerticalOptions" Value="FillAndExpand"/>
                <Setter Property="HorizontalOptions" Value="FillAndExpand"/>
                <Setter Property="Margin" Value="0"/>
            </Style>
            <Style TargetType="temp:ImageV" BasedOn="{StaticResource ImageStyle}"/>

            <!--BoxViewStyle-->
            <Style x:Key="BoxViewStyle" TargetType="BoxView">
                <Setter Property="Color" Value="{DynamicResource ClrSilver}" />
                <Setter Property="HeightRequest" Value="1"/>
            </Style>
            <Style TargetType="temp:BoxViewV" BasedOn="{StaticResource BoxViewStyle}">
            </Style>

            <!--PanelVStyle-->
            <Style x:Key="PanelVStyle" TargetType="temp:PanelV">
                <Setter Property="PnId" Value="{Binding Id}"/>
                <Setter Property="PnNo" Value="{Binding No}"/>
                <Setter Property="PnTitle1" Value="{Binding Title1}"/>
                <Setter Property="PnTitle2" Value="{Binding Title2}"/>
                <Setter Property="PnViewTitle1" Value="{Binding IsViewTitle1}"/>
                <Setter Property="PnViewTitle2" Value="{Binding IsViewTitle2}"/>
                <Setter Property="PnSelected" Value="{Binding IsSelected}"/>
                <Setter Property="PnVisible" Value="{Binding IsVisible}"/>
                <Setter Property="PnReadOnly" Value="{Binding IsReadOnly}"/>
                <Setter Property="PnImageSource" Value="{Binding ImageSource}"/>

                <Setter Property="PnMargin" Value="0"/>
                <Setter Property="PnPadding" Value="5"/>
                <Setter Property="PnBackgroundColor" Value="Transparent"/>
                <Setter Property="PnBorderColor" Value="{DynamicResource ClrSilver}"/>
                <Setter Property="PnTextColor1" Value="{DynamicResource ClrTextHint}"/>
                <Setter Property="PnTextColor2" Value="{DynamicResource ClrTextNormal}"/>
                <Setter Property="PnTextColor3" Value="{DynamicResource ClrTextNormal}"/>

                <Setter Property="PnSelectCommand" Value="{Binding SelectCommand}"/>
                <Setter Property="PnActionCommand" Value="{Binding ActionCommand}"/>

                <Style.Triggers>
                    <!--Select-->
                    <Trigger TargetType="temp:PanelV" Property="PnSelected" Value="True">
                        <Setter Property="PnBorderColor" Value="{StaticResource ClrPrimary}" />
                    </Trigger>
                </Style.Triggers>
            </Style>

            <!--InputTStyle-->
            <Style x:Key="InputTStyle" TargetType="temp:InputT" BasedOn="{StaticResource PanelVStyle}">
                <Setter Property="PnTextHint1" Value="{Binding TextHint1}"/>
                <Setter Property="PnTextHint2" Value="{Binding TextHint2}"/>
                <Setter Property="PnTextHelper" Value="{Binding TextHelper}"/>
                <Setter Property="PnIcon" Value="{Binding Icon}"/>
                <Setter Property="PnFontAttributes" Value="{Binding FontAttributes}"/>
                <Setter Property="PnReadOnly" Value="{Binding IsReadOnly}"/>
                <Setter Property="PnFocus" Value="{Binding IsFocus}"/>
                <Setter Property="PnViewTextHint1" Value="{Binding IsViewTextHint1}"/>
                <Setter Property="PnViewTextHint2" Value="{Binding IsViewTextHint2}"/>
                <Setter Property="PnViewIcon" Value="{Binding IsViewIcon}"/>
                <Setter Property="PnViewLine" Value="{Binding IsViewLine}"/>

                <Setter Property="PnMargin" Value="0,0,0,5"/>
                <Setter Property="PnBorderColor" Value="Transparent"/>
            </Style>

            <!--InputTextTStyle-->
            <Style x:Key="InputTextTStyle" TargetType="temp:InputTextT" BasedOn="{StaticResource InputTStyle}">
                <Setter Property="PnText" Value="{Binding Text, Mode=TwoWay}"/>
                <Setter Property="PnTextAlignment" Value="{Binding TextAlignment}"/>
                <Setter Property="PnTextLength" Value="{Binding TextLength}"/>
                <Setter Property="PnMaxLength" Value="{Binding MaxLength}"/>
                <Setter Property="PnKeyboard" Value="{Binding Keyboard}"/>
                <Setter Property="PnPassword" Value="{Binding IsPassword}"/>
                <Setter Property="PnTextChangedCommand" Value="{Binding TextChangedCommand}"/>
                <Setter Property="PnLeaveCommand" Value="{Binding LeaveCommand}"/>
                <Setter Property="PnEnterCommand" Value="{Binding EnterCommand}"/>
            </Style>
            <Style TargetType="temp:InputText1V" BasedOn="{StaticResource InputTextTStyle}"/>
            <Style TargetType="temp:InputText2V" BasedOn="{StaticResource InputTextTStyle}"/>
            <Style TargetType="temp:InputText3V" BasedOn="{StaticResource InputTextTStyle}"/>
            <Style TargetType="temp:InputText4V" BasedOn="{StaticResource InputTextTStyle}">
                <Setter Property="PnPadding" Value="10,5,10,5"/>
                <Setter Property="PnBorderColor" Value="{DynamicResource ClrGray}"/>
            </Style>
            <Style TargetType="temp:InputTextSearchV" BasedOn="{StaticResource InputTextTStyle}">
                <Setter Property="PnMargin" Value="0,0,0,-5"/>
                <Setter Property="PnBackgroundColor" Value="{DynamicResource ClrSilver}"/>
                <Setter Property="PnSearchCommand" Value="{Binding SearchCommand}"/>
                <Setter Property="PnSearchEnterCommand" Value="{Binding SearchEnterCommand}"/>
                <Setter Property="PnSearchDelayCommand" Value="{Binding SearchDelayCommand}"/>
            </Style>


            <!--InputDateTStyle-->
            <Style x:Key="InputDateTStyle" TargetType="temp:InputDateT" BasedOn="{StaticResource InputTStyle}">
                <Setter Property="PnValue" Value="{Binding Value, Mode=TwoWay}"/>
                <Setter Property="PnValueMin" Value="{Binding ValueMin}"/>
                <Setter Property="PnValueMax" Value="{Binding ValueMax}"/>
                <Setter Property="PnValueChangedCommand" Value="{Binding ValueChangedCommand}"/>
            </Style>
            <Style TargetType="temp:InputDateV" BasedOn="{StaticResource InputDateTStyle}"/>

            <!--InputSwitchTStyle-->
            <Style x:Key="InputSwitchTStyle" TargetType="temp:InputSwitchT" BasedOn="{StaticResource InputTStyle}">
                <Setter Property="PnOn" Value="{Binding IsOn, Mode=TwoWay}"/>
                <Setter Property="PnCheckedChangedCommand" Value="{Binding CheckedChangedCommand}"/>
            </Style>
            <Style TargetType="temp:InputSwitchV" BasedOn="{StaticResource InputSwitchTStyle}"/>

            <!--InputPickerTStyle-->
            <Style x:Key="InputPickerTStyle" TargetType="temp:InputPickerT" BasedOn="{StaticResource InputTStyle}">
                <Setter Property="PnText" Value="{Binding Text}"/>
                <Setter Property="PnIcon2" Value="{Binding Icon2}"/>
                <Setter Property="PnPickerChangedCommand" Value="{Binding PickerChangedCommand}"/>
            </Style>
            <Style TargetType="temp:InputPickerV" BasedOn="{StaticResource InputPickerTStyle}"/>


            <!--InputRateTStyle-->
            <Style x:Key="InputRateTStyle" TargetType="temp:InputRateT" BasedOn="{StaticResource InputTStyle}">
                <Setter Property="PnRateVM" Value="{Binding RateVM}"/>
            </Style>
            <Style TargetType="temp:InputRateV" BasedOn="{StaticResource InputRateTStyle}"/>
            
            
            <!--PINTStyle-->
            <Style x:Key="PINTStyle" TargetType="temp:PINT" BasedOn="{StaticResource PanelVStyle}">
                <Setter Property="PnInputPIN1VM" Value="{Binding InputPIN1VM}"/>
                <Setter Property="PnInputPIN2VM" Value="{Binding InputPIN2VM}"/>
                <Setter Property="PnInputPIN3VM" Value="{Binding InputPIN3VM}"/>
                <Setter Property="PnInputPIN4VM" Value="{Binding InputPIN4VM}"/>
                <Setter Property="PnInputPIN5VM" Value="{Binding InputPIN5VM}"/>
                <Setter Property="PnInputPIN6VM" Value="{Binding InputPIN6VM}"/>
                <Setter Property="PnBorderColor" Value="Transparent"/>
            </Style>
            <Style TargetType="temp:PINV" BasedOn="{StaticResource PINTStyle}"/>

            <!--MenuTStyle-->
            <Style x:Key="MenuTStyle" TargetType="temp:MenuT" BasedOn="{StaticResource PanelVStyle}">
                <Setter Property="PnIcon" Value="{Binding Icon}"/>
                <Setter Property="PnViewIcon" Value="{Binding IsViewIcon}"/>
                <Setter Property="PnViewSpace" Value="{Binding IsViewSpace}"/>
            </Style>
            <Style TargetType="temp:Menu1V" BasedOn="{StaticResource MenuTStyle}">
            </Style>
            <Style TargetType="temp:Menu2V" BasedOn="{StaticResource MenuTStyle}">
            </Style>
            <Style TargetType="temp:Menu3V" BasedOn="{StaticResource MenuTStyle}">
                <Setter Property="PnBorderColor" Value="Transparent"/>
            </Style>

            <DataTemplate x:Key="MenuGrid">
                <temp:Menu1V/>
            </DataTemplate>
            <DataTemplate x:Key="MenuList">
                <temp:Menu2V/>
            </DataTemplate>
            <DataTemplate x:Key="MenuT1">
                <temp:Menu3V/>
            </DataTemplate>


