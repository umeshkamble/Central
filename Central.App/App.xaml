                <Setter Property="PnMode" Value="{Binding Mode}"/>
                <Setter Property="PnTgl" Value="{Binding Tgl}"/>
                <Setter Property="PnNoReferensis" Value="{Binding NoReferensis}"/>
                <Setter Property="PnNama" Value="{Binding Nama}"/>
                <Setter Property="PnDeskripsi" Value="{Binding Deskripsi}"/>
                <Setter Property="PnPSumVM" Value="{Binding PSumVM}"/>
