<?xml version="1.0" encoding="utf-8" ?>
<temp:VariantT  xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                xmlns:temp="clr-namespace:Central.App.Templates"
                x:Class="Central.App.Templates.VariantInfoV">

    <temp:VariantT.Body>
        <Grid
            RowSpacing="0"
            ColumnSpacing="0">

            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>

            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <!--Input-->
            <temp:InputText2V
               BindingContext="{Binding PnInputVM}"/>
        </Grid>
    </temp:VariantT.Body>
    
    
</temp:VariantT>