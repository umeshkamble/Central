                <Setter Property="PnAddressListVM" Value="{Binding AddressListVM}"/>
                <Setter Property="PnPhoneListVM" Value="{Binding PhoneListVM}"/>

                <Setter Property="PnBorderColor" Value="Transparent"/>
                <Setter Property="PnMargin" Value="0"/>
                <Setter Property="PnPadding" Value="0"/>
            </Style>

            
            
